# Harbor Credentials
# Copy this file to .env and fill in your actual passwords
# .env is gitignored and will not be committed

# Harbor Admin (default password, change via UI after first login)
HARBOR_ADMIN_PASSWORD=Harbor12345

# Database and Redis passwords
HARBOR_DATABASE_PASSWORD=root123
HARBOR_REDIS_PASSWORD=

# Harbor Registry Configuration
# Harbor registry URL (used for docker login and image pushes)
HARBOR_REGISTRY_URL=harbor.dataknife.net

# Robot Account for CI/CD builds
# Create robot account in Harbor UI: Administration -> Robot Accounts
# Or use: ./scripts/create-harbor-robot-account.sh
HARBOR_ROBOT_ACCOUNT_NAME=ci-builder
HARBOR_ROBOT_ACCOUNT_SECRET=CHANGE_ME_ROBOT_SECRET
# Robot account full name format: robot$<project>+<name>
# Example: robot$library+ci-builder (for project 'library', account 'ci-builder')
HARBOR_ROBOT_ACCOUNT_FULL_NAME=robot$library+ci-builder
